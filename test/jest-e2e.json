{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/mmmhelpers/(.*)": "<rootDir>/../libs/mmmhelpers/src/$1",
    "@app/mmmhelpers": "<rootDir>/../libs/mmmhelpers/src",
    "@app/helpers/(.*)": "<rootDir>/../libs/helpers/src/$1",
    "@app/helpers": "<rootDir>/../libs/helpers/src",
    "@mmm/authelpers/(.*)": "<rootDir>/../libs/authelpers/src/$1",
    "@mmm/authelpers": "<rootDir>/../libs/authelpers/src"
  }
}